<?xml version='1.0' encoding='utf-8'?>
<widget id="dev.mrkprdo.clientgpt" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ClientGPT</name>
    <description>A GPT client-side model agnostic app</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="Scheme" value="https" />
    <preference name="usesCleartextTraffic" value="true" />
    <content src="index.html" />
    <allow-navigation href="about:*" />
</widget>
